    <!-- Antardasha Interpretation -->
    <string name="antardasha_interp_header">%1$s-%2$s: %2$s को अन्तरदशा (महादशाबाट %3$d घर)। स्वामी %4$s ले %2$s विषयहरू सक्रिय गर्दछ।</string>
    <string name="antardasha_interp_house1">आत्म-केन्द्रित अवधि, व्यक्तिगत पहल।</string>
    <string name="antardasha_interp_house2">धन, परिवार, बोलीमा ध्यान।</string>
    <string name="antardasha_interp_house3">साहस, दाजुभाइ, सञ्चार सक्रिय।</string>
    <string name="antardasha_interp_house4">घर, आमा, सम्पत्ति मामिला।</string>
    <string name="antardasha_interp_house5">सन्तान, रचनात्मकता, रोमान्स हाइलाइट।</string>
    <string name="antardasha_interp_house6">स्वास्थ्य, प्रतिस्पर्धा, सेवा मामिला।</string>
    <string name="antardasha_interp_house7">साझेदारी, विवाह, सार्वजनिक कारोबार।</string>
    <string name="antardasha_interp_house8">रूपान्तरण, उत्तराधिकार, अनुसन्धान।</string>
    <string name="antardasha_interp_house9">भाग्य, उच्च शिक्षा, आध्यात्मिकता।</string>
    <string name="antardasha_interp_house10">करियर, स्थिति, सार्वजनिक मान्यता।</string>
    <string name="antardasha_interp_house11">लाभ, मित्रता, आकांक्षा पूरा।</string>
    <string name="antardasha_interp_house12">खर्च, आध्यात्मिकता, विदेशी जडान।</string>

    <!-- Overall Dasha Interpretation -->
    <string name="dasha_interp_overview">%1$s लग्नका लागि चर दशा %2$s दिशामा सञ्चालन हुन्छ। तपाईंको आत्मकारक %3$s हो, जसले तपाईंको आत्माको प्राथमिक पाठ %4$s मा समावेश रहेको संकेत गर्दछ।</string>
